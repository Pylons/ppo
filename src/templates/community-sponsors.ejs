<%- include partials/top %>

<div class="page">
  <section class="section-content">
    <div class="container">
      <div class="col-lg-10 col-lg-offset-1">
        <h1 class="title text-blue">Sponsors</h1>
         <section id="sponsors">
          <h3>We are grateful for the support from our sponsors</h3>

      <div class="row sponsor-items">
        <% context.sponsors.forEach(function(item){ %>
            <div class="col-xs-12 col-sm-6">
              <div class="sponsor-item">
                <h4>
                    <% if (item.websiteUrl != '') { %><a href="<%= item.websiteUrl %>"><% } %>
                    <%= item.name %>
                    <% if (item.websiteUrl != '') { %></a><% } %>
                  <% if (item.logo != '') { %>
                    <% if (item.websiteUrl != '') { %><a href="<%= item.websiteUrl %>"><% } %>
                    <img src="img/<%= item.logo %>" class="pull-right">
                    <% if (item.websiteUrl != '') { %></a><% } %>
                  <% } %>
                </h4>
                <p class="sponsor-description"><%- item.description %></p>
              </div>
            </div>
        <% }); %>
      </div>


        </section>
      </div>
    </div>
  </section>
</div>
<%- include partials/bottom %>
